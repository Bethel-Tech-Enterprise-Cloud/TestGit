% Draw a gradient
200 {
    /y exch def
    y 0 200 0 moveto
    200 y lineto
    y 200 200 lineto
    y 200 0 lineto
    closepath
    (1.0 0.0 0.0) setrgbcolor  % Red
    fill
    1 setgray
} repeat

